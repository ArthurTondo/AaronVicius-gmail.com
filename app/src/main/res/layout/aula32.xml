<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:shrinkColumns="*"
        android:stretchColumns="*"
        android:background="#000000"
        >



        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"

            >

            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:src="@drawable/onibus"
                android:background="#000000"
                />
            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:src="@drawable/aviao"
                android:background="#000000"
                />
            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:background="#000000"
                android:src="@drawable/bairros"
                />
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"

            >

            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:background="#000000"
                android:src="@drawable/turismo"
                />
            <ImageButton
                android:layout_height="wrap_content"
                android:background="#000000"
                android:layout_width="match_parent"
                android:src="@drawable/telefone"
                />
            <ImageButton
                android:layout_height="wrap_content"
                android:background="#000000"
                android:layout_width="match_parent"
                android:src="@drawable/taxi"
                />
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="wrap_content">

            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:background="#000000"
                android:src="@drawable/cultura"
                />
            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:src="@drawable/feriados"
                android:background="#000000"
                />
            <ImageButton
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:background="#000000"
                android:src="@drawable/sac"
                />
        </TableRow>

    </TableLayout>

</RelativeLayout>